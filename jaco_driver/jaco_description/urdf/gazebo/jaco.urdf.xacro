				<mesh filename="package://jaco_description/meshes/joint_1.dae" 
				scale="0.001 0.001 0.001"/>     
			</geometry> 
		</collision>
	</link>
	<gazebo reference="jaco_base_link">
		<material>Gazebo/Yellow</material>
		<turnGravityOff>true</turnGravityOff>
	</gazebo>

